<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RatGame.LevelCreationMenu">
   <children>
      <Button id="btnEdit" fx:id="btnEdit" layoutX="253.0" layoutY="352.0" mnemonicParsing="false" onAction="#editLevel" text="Edit Level" />
      <Button id="btnBack" fx:id="btnBack" layoutX="524.0" layoutY="14.0" mnemonicParsing="false" onAction="#runReturn" text="back" />
      <Slider id="slSizeX" fx:id="slSizeX" blockIncrement="1.0" layoutX="196.0" layoutY="184.0" max="25.0" min="3.0" prefHeight="16.0" prefWidth="208.0" showTickMarks="true" value="1.0" />
      <Slider id="slSizeY" fx:id="slSizeY" blockIncrement="1.0" layoutX="198.0" layoutY="236.0" max="25.0" min="3.0" prefHeight="16.0" prefWidth="208.0" showTickMarks="true" value="1.0" />
      <Label layoutX="257.0" layoutY="102.0" text="Level Name" textAlignment="CENTER" />
      <Label layoutX="276.0" layoutY="161.0" text="X Size" textAlignment="CENTER" />
      <Label layoutX="277.0" layoutY="212.0" text="Y Size" textAlignment="CENTER" />
      <ComboBox fx:id="comName" editable="true" layoutX="198.0" layoutY="117.0" prefHeight="25.0" prefWidth="208.0" />
   </children>
</AnchorPane>
